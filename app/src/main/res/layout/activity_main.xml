<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/tab"
        android:layout_width="wrap_content"
        android:text="tablayout"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/next1"
        android:layout_height="wrap_content"/>


    <Button
        android:id="@+id/next"
        android:layout_width="wrap_content"
        android:text="bussnessdeails"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_height="wrap_content"/>

    <Button
        android:id="@+id/next1"
        android:layout_width="wrap_content"
        android:text="busssnessdetailsuntick"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/next"
        android:layout_height="wrap_content"/>
    <Button
        android:id="@+id/adhar"
        android:layout_width="wrap_content"
        android:text="addharotp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tab"
        android:layout_height="wrap_content"/>

    <Button
        android:id="@+id/security_button"
        android:layout_width="wrap_content"
        android:text="security"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/adhar"
        android:layout_height="wrap_content"/>
    <Button
        android:id="@+id/splash_button"
        android:layout_width="wrap_content"
        android:text="splash"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/security_button"
        android:layout_height="wrap_content"/>

</androidx.constraintlayout.widget.ConstraintLayout>